// In this method I covered Abstraction, classes and objects, Interface
interface C1{
	final int a = 10;
	void display();
}


public class Circle implements C1{
	
	public void display() {
		System.out.println("Radius of Circle is ");
	}
	
  public float area(){
  float radius=10f;
  return 3.14f * radius * radius;
  }
  public float diameter(){
  float radius=10f;
  return radius + radius;
  }
  public float circumference(float radius){
  return 2 * 3.14f * radius;
  }
 public static void main(String[] args){
   Circle c1 = new Circle();
   Circle c2 = new Circle();
   Circle c3 = new Circle();
   Circle c4 = new Circle();
   c4.display();
   System.out.println(a);
   System.out.println("Area of Circle is " + c1.area() + "\n" + "Area of diameter is "
                            + c2.diameter() + "\n" + "circumference of circle is " + 
		   c3.circumference(10f));
   }
}
